# Domains to monitor (comma-separated for multiple domains)
# Example: DOMAINS=example.com,google.com,github.com
DOMAINS=example.com

# Telegram Bot Token (get from @BotFather) [optional]
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# Telegram Chat ID (your chat ID or group chat ID) [optional]
TELEGRAM_CHAT_ID=your_telegram_chat_id_here

# Discord Webhook URL (get from Discord channel settings) [optional]
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/your_webhook_url_here

# Monitoring interval in seconds (default: 3600 = 1 hour)
MONITORING_INTERVAL=3600 
